{
  "$schema": "https://json.schemastore.org/vss-extension.json",
  "manifestVersion": 1,
  "id": "string-kit",
  "version": "4.4.1",
  "name": "Strings Kit",
  "description": "Strings Kit provides tasks to manupulate strings contents from different kind sources.",
  "publisher": "alelltech",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/vss.png"
  },
  "screenshots": [],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/alelltech/azdo-string-kit/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/alelltech/azdo-string-kit"
  },
  "tags": [
    "azure devops",
    "pipeline",
    "task",
    "base64",
    "regex replace",
    "nunjucks",
    "underscore.string"
  ],
  "files": [
    {
      "path": "BuildTasks/NunjucksInline"
    },
    {
      "path": "BuildTasks/Nunjucks"
    },
    {
      "path": "BuildTasks/RegexReplace"
    },
    {
      "path": "BuildTasks/Base64"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "NunjucksInline",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "BuildTasks/NunjucksInline"
      }
    },
    {
      "id": "Nunjucks",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "BuildTasks/Nunjucks"
      }
    },
    {
      "id": "RegexReplace",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "BuildTasks/RegexReplace"
      }
    },
    {
      "id": "Base64",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "BuildTasks/Base64"
      }
    }
  ]
}
